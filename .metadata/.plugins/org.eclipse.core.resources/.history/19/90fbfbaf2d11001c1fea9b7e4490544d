
public class RGBColor {
	private int red ,green ,blue ;
	
	public RGBColor () {} 
	
	public RGBColor(int r , int g ,int b) {
		this.red = r ;
		this.green = g ;
		this.blue = b;
	}
	
	public int getRed () {
		return this.red ;
	}
	
	public int getGreen () {
		return this.green ;
	}
	
	public int getBlue () {
		return this.blue ;
	}
}
